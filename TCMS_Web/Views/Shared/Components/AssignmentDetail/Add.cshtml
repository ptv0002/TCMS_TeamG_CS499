@model Models.AssignmentDetail

@{
    ViewData["Title"] = "Add to Assignment Detail";
}

<h1>Create</h1>

<h4>ShippingAssignment</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Add">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Id" class="control-label"></label>
                <select asp-for="Id" class="form-control" asp-items="ViewBag.Id"></select>
            </div>
            <div class="form-group">
                <label asp-for="OrderInfo" class="control-label"></label>
                <select asp-for="OrderInfo" class="form-control" asp-items="ViewBag.OrderInfo"></select>
            </div>
            <div class="form-group">
                <label asp-for="InShipping" class="control-label"></label>
                <input asp-for="InShipping" class="form-control" />
                <span asp-validation-for="InShipping" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ArrivalConfirm" class="control-label"></label>
                <input asp-for="ArrivalConfirm" class="form-control" />
                <span asp-validation-for="ArrivalConfirm" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ArrivalTime" class="control-label"></label>
                <input asp-for="ArrivalTime" class="form-control" />
                <span asp-validation-for="ArrivalTime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Status" class="control-label"></label>
                <input asp-for="Status" class="form-control" />
                <span asp-validation-for="Status" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ShippingAssignmentId" class="control-label"></label>
                <select asp-for="ShippingAssignmentId" class="form-control" asp-items="ViewBag.ShippingAssignmentId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Add" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>


